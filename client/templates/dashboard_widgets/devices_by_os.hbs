<div class="block">
	<div class="block-header">
		<div class="block-header-title pull-left">
			{{title}}
		</div>
	</div>

	<div class="container-fluid raw-fluid block-body" >
		<div id="chart-{{index}}" style="height: 400px;"></div>
	</div>

</div>

<script type="text/javascript">
	var chartData = [{
		label : "Series1",
		data : 10
	}, {
		label : "Series2",
		data : 30
	}, {
		label : "Series3",
		data : 90
	}, {
		label : "Series4",
		data : 70
	}, {
		label : "Series5",
		data : 80
	}, {
		label : "Series6",
		data : 110
	}];

	$.plot('#chart-{{index}}', chartData, {
		series : {
			pie : {
				show : true
			}
		}
	});

</script>
