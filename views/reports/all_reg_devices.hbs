<div class="container">

	<div class="raw-fluid">
		<div class="span12">

			<div class="raw-fluid">
				<div class="block">
					<div class="block-header">
						<div class="block-header-title pull-left">
							All registered devices within a period
						</div>
						
							<div class="pull-right">
									

								</div>

					</div>
					
					
					

					<div class="block-body block-body container-fluid raw-fluid">

						<div class="raw-fluid" style="margin-left: -25px">
							
							<form method="post" enctype="multipart/form-data">

							<div class="span2 operation-filter">
								<div class="control-group">
									<label class="control-label" for="inputLastName">Start Date</label>
									<div class="controls">
										   
										 <div class="input-append date datepicker"  data-date="12-02-2012" data-date-format="dd-mm-yyyy">
				<input id="startdate" name="startdate" class="span2" size="16" type="text" value="12-02-2012">
				<span class="add-on"><i class="icon-th"></i></span>
			  </div>
									  
									</div>
								</div>
							</div>
							
							
							<div class="span2 operation-filter">
								<div class="control-group">
									<label class="control-label" for="inputLastName">End Date</label>
									<div class="controls">
										   
										 <div class="input-append date datepicker"  data-date="12-02-2012" data-date-format="dd-mm-yyyy">
				<input id="enddate" name="enddate" class="span2" size="16" type="text" value="12-02-2012">
				<span class="add-on"><i class="icon-th"></i></span>
			  </div>
									  
									</div>
								</div>
							</div>

							<div class="span2 operation-filter">
								<div class="control-group">
									<label class="control-label" for="inputEmail">Device Type</label>
									<div class="controls">
										<select id="platform" name="platform"  >
											<option value="0">--All--</option>
											<option value="1">Android</option>
											<option value="2">iOS</option>
										</select>
									</div>
								</div>
							</div>

							
							<div class="span2 operation-filter">								
								<div class="control-group">
									<label class="control-label" for="inputPassword" >&nbsp;</label>
									<div class="controls">
										<button class="btn btn-orange" type="sumbit" >Find</button>
									</div>
								</div>
							</div>
							
							</form>
							
							
						</div>		
												

						<div class="raw">

							<div class="span11">						

								<table cellpadding="0" cellspacing="0" border="0"
								class="table table-striped table-bordered" id="main-table">
									<thead>
										<tr>											
											<th style="display: none">Id</th>
											<th>IMEI</th>
											<th>Username</th>
											<th>Platform</th>
											<th>OS version</th>
											<th>Name</th>											
										</tr>
									</thead>
									<tbody>

									</tbody>									
								</table>
								
							</div>
						</div>

					</div>
				</div>
			</div>

		</div>

	</div>

</div>

